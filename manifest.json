{
  "name": "Shopezy",
  "version": "1.7",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "shop.html",
    "default_icon": {
      "16": "icon16.png",
      "24": "icon24.png",
      "32": "icon32.png",
      "64": "icon64.png",
      "128": "icon128.png"
    }
  },
  "background": {
    "scripts": ["popup.js"]
  },
  "permissions": ["tabs", "activeTab"]
}